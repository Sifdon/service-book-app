<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.srinathv.servicebook.LoggedIn">

    <ImageView
        android:id="@+id/img1"
        android:layout_width="64dp"
        android:layout_height="57dp"
        android:src="@drawable/logo"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:id="@+id/indianmistry"
        android:text="Indian Mistrry"
        android:textAlignment="center"
        android:layout_alignRight="@+id/img1"
        android:textSize="40sp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toRightOf="@+id/img1"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.511"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageView
        android:id="@+id/imgmenu"
        android:layout_width="56dp"
        android:layout_height="51dp"
        app:srcCompat="@drawable/menuicon"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.166"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageView
        android:id="@+id/imgelectrician"
        android:layout_width="84dp"
        android:layout_height="82dp"
        app:srcCompat="@drawable/electrician"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtelec"
        app:layout_constraintVertical_bias="0.992"
        android:layout_marginLeft="39dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="39dp" />

    <ImageView
        android:id="@+id/imgplumber"
        android:layout_width="85dp"
        android:layout_height="89dp"
        app:srcCompat="@drawable/plumber"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="117dp"
        app:layout_constraintRight_toLeftOf="@+id/imgcar"
        app:layout_constraintLeft_toRightOf="@+id/imgelectrician"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtplumber"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imgcar"
        android:layout_width="100dp"
        android:layout_height="93dp"
        app:srcCompat="@drawable/car"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtcar"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="32dp" />

    <ImageView
        android:id="@+id/imgbike"
        android:layout_width="102dp"
        android:layout_height="86dp"
        app:srcCompat="@drawable/bike"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="9dp"
        app:layout_constraintBottom_toTopOf="@+id/txtbike"
        android:layout_marginEnd="32dp" />

    <ImageView
        android:id="@+id/imgclean"
        android:layout_width="89dp"
        android:layout_height="85dp"
        app:srcCompat="@drawable/cleaning"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="34dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/txtclean"
        android:layout_marginStart="34dp" />

    <ImageView
        android:id="@+id/imgphoto"
        android:layout_width="82dp"
        android:layout_height="84dp"
        app:srcCompat="@drawable/photos"
        app:layout_constraintRight_toLeftOf="@+id/imgbike"
        android:layout_marginRight="17dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toTopOf="@+id/txtphoto"
        android:layout_marginEnd="17dp" />

    <ImageView
        android:id="@+id/imgother"
        android:layout_width="145dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/other"
        android:layout_marginLeft="39dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="6dp"
        app:layout_constraintBottom_toTopOf="@+id/txtother"
        android:layout_marginStart="39dp" />

    <ImageView
        android:id="@+id/imgdj"
        android:layout_width="136dp"
        android:layout_height="96dp"
        app:srcCompat="@drawable/dj"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtdj"
        android:layout_marginEnd="32dp" />

    <TextView
        android:id="@+id/txtelec"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Electrician"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toTopOf="@+id/imgclean"
        app:layout_constraintRight_toLeftOf="@+id/txtplumber"
        android:layout_marginRight="43dp"
        android:layout_marginEnd="43dp" />

    <TextView
        android:id="@+id/txtplumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Plumber"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toTopOf="@+id/imgphoto"
        app:layout_constraintRight_toLeftOf="@+id/txtcar"
        android:layout_marginRight="43dp"
        android:layout_marginEnd="43dp" />

    <TextView
        android:id="@+id/txtcar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Car Service"
        android:layout_marginRight="52dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toTopOf="@+id/imgbike"
        android:layout_marginEnd="52dp" />

    <TextView
        android:id="@+id/txtclean"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cleaning"
        android:layout_marginLeft="52dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imgother"
        android:layout_marginStart="52dp" />

    <TextView
        android:id="@+id/txtphoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Photographer"
        android:layout_marginBottom="7dp"
        app:layout_constraintBottom_toTopOf="@+id/imgother"
        app:layout_constraintRight_toLeftOf="@+id/txtbike"
        android:layout_marginRight="33dp"
        android:layout_marginEnd="33dp" />

    <TextView
        android:id="@+id/txtbike"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bike Service"
        android:layout_marginRight="46dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imgdj"
        android:layout_marginEnd="46dp" />

    <TextView
        android:id="@+id/txtother"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AC, fridge and other"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/txtdj"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/txtdj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DJ booking"
        android:layout_marginRight="72dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="72dp" />

